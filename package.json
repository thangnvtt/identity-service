{
  "name": "user-identity-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "main": "dist/function.js",
  "types": "dist/function.d.ts",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged --allow-empty",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:dev": "prisma migrate dev",
    "generate:prisma": "prisma generate",
    "make-migration": "yarn ts prisma/generate_schema.ts && yarn prisma migrate dev --create-only --schema prisma/migrate.prisma",
    "generate:view": "yarn ts prisma/generate_view.ts",
    "migrate": "yarn ts prisma/generate_schema.ts && yarn prisma migrate deploy --schema prisma/migrate.prisma && yarn generate:view",
    "studio": "prisma studio",
    "admin:create-user": "yarn build && node dist/src/scripts/create-admin-user"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^3.2.1",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "@prisma/client": "^4.6.0",
    "@types/multer": "^1.4.7",
    "axios": "^0.27.2",
    "cache-manager": "4.0.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-util": "^7.1.5",
    "firebase": "^9.14.0",
    "firebase-admin": "^11.2.1",
    "humps": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "moment": "^2.29.4",
    "node-jose": "^2.1.1",
    "otplib": "^12.0.1",
    "prisma": "^4.6.0",
    "qrcode": "^1.5.1",
    "redis": "^4.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/testing": "^9.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/node-jose": "^1.1.10",
    "@types/qrcode": "^1.5.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.132.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
